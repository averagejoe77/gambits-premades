        "type": "script",
        "scope": "global",
        "command": "if(args[0].macroPass === \"postActiveEffects\") {\n    let itemData = {\n        \"name\": \"Leiloch’s Interminable Yarn - Pull Away\",\n        \"type\": \"feat\",\n        \"img\": macroItem.img,\n        \"system\": {\n            \"activation\": {\n                \"type\": \"special\",\n                \"cost\": null,\n                \"condition\": \"\"\n            },\n            \"target\": {\n                \"value\": null,\n                \"width\": null,\n                \"units\": \"\",\n                \"type\": \"self\",\n                \"prompt\": true\n            },\n            \"range\": {\n                \"value\": null,\n                \"long\": null,\n                \"units\": \"self\"\n            },\n            \"actionType\": \"save\",\n            \"save\": {\n                \"ability\": \"wis\",\n                \"dc\": actor.system.attributes.spelldc,\n                \"scaling\": \"flat\"\n            }\n        },\n        \"effects\": [\n            {\n                \"icon\": macroItem.img,\n                \"duration\": {\n                    \"seconds\": 60\n                },\n                \"disabled\": false,\n                \"name\": \"Leiloch’s Interminable Yarn - Unconscious\",\n                \"changes\": [\n                    {\n                        \"key\": \"macro.StatusEffect\",\n                        \"mode\": 0,\n                        \"value\": \"Convenient Effect: Unconscious\",\n                        \"priority\": 20\n                    }\n                ],\n                \"transfer\": false\n            }\n        ],\n        \"flags\": {\n            \"midiProperties\": {\n                \"magiceffect\": true\n            },\n            \"midi-qol\": {\n                \"onUseMacroName\": \"[postActiveEffects]ItemMacro\"\n            },\n            \"dae\": {\n                \"macro\": {\n                    \"name\": \"Leiloch’s Interminable Yarn - Pull Away\",\n                    \"img\": macroItem.img,\n                    \"type\": \"script\",\n                    \"scope\": \"global\",\n                    \"command\": \"let effectData = actor.appliedEffects.find(e => e.name === 'Leiloch’s Interminable Yarn'); await effectData.delete(); await macroItem.delete();\"\n                }\n            }\n        }\n    }\n\n    for (let target of workflow.targets) {\n        await target.actor.createEmbeddedDocuments(\"Item\", [itemData]);\n    }\n}\n\nif(args[0] === \"off\") {\n    let itemData = actor.items.getName(\"Leiloch’s Interminable Yarn - Pull Away\");\n    await itemData.delete();\n}",
