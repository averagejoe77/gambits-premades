    "itemacro": {
      "macro": {
        "name": "Torch",
        "type": "script",
        "scope": "global",
        "command": "//Last Updated: 9/14/2023\n//Author: EskieMoh#2969\n\n//Set torch or lantern\nlet lightSource = \"torch\"\n\nif (lightSource == \"torch\"){\nfile = \"https://i.imgur.com/1U1eb9t.png\"\n}  \nelse if (lightSource == \"lantern\"){\nfile = \"https://i.imgur.com/3wFQD02.png\"\n}\n\nlet lightDim = token.document.light.dim;\nlet lightBright = token.document.light.bright;\n\nif (lightDim == 0 && lightBright == 0){\nnew Sequence()\n\n.effect()\n.delay(150)\n.name(`${token.document.name} ${lightSource}`)\n.file(\"jb2a.impact.002.orange\")\n.atLocation(token)\n.attachTo(token, {followRotation: true, local: true})\n.scaleToObject(0.9, {considerTokenScale: true})\n.spriteOffset({x:0.525*token.document.width, y:-0.05*token.document.width}, {gridUnits:true})\n.spriteRotation(45)\n.zIndex(0.1)\n.playIf(() => {\nreturn lightSource == \"torch\";\n})\n\n.effect()\n.name(`${token.document.name} ${lightSource}`)\n.file(file)\n.atLocation(token)\n.attachTo(token, {followRotation: true, local: true})\n.scaleToObject(1.2, {considerTokenScale: true})\n.scaleIn(0, 500, {ease: \"easeOutElastic\"})\n.scaleOut(0, 250, {ease: \"easeOutCubic\"})\n.spriteOffset({x:0.35*token.document.width, y:0.1*token.document.width}, {gridUnits:true})\n.animateProperty(\"sprite\", \"rotation\", { from: 15, to: -15, duration: 300, ease: \"easeInOutBack\"})\n.animateProperty(\"sprite\", \"rotation\", { from: 0, to: 30, duration: 250, delay: 200, ease: \"easeOutBack\"})\n.persist()\n.zeroSpriteRotation()\n.playIf(() => {\nreturn lightSource == \"torch\";\n})\n\n.effect()\n.delay(250)\n.name(`${token.document.name} ${lightSource}`)\n.file(\"jb2a.flames.01.orange\")\n.atLocation(token)\n.attachTo(token, {followRotation: true, local: true})\n.scaleToObject(1, {considerTokenScale: true})\n.spriteOffset({x:0.5*token.document.width, y:-0.05*token.document.width}, {gridUnits:true})\n.persist()\n.spriteRotation(45)\n.zIndex(0.1)\n.waitUntilFinished(-500)\n.playIf(() => {\nreturn lightSource == \"torch\";\n})\n\n.effect()\n.name(`${token.document.name} ${lightSource}`)\n.file(file)\n.atLocation(token)\n.attachTo(token, {followRotation: true, local: true})\n.scaleToObject(1, {considerTokenScale: true})\n.scaleIn(0, 500, {ease: \"easeOutElastic\"})\n.scaleOut(0, 250, {ease: \"easeOutCubic\"})\n.spriteOffset({x:0.35*token.document.width, y:0.1*token.document.width}, {gridUnits:true})\n.animateProperty(\"sprite\", \"rotation\", { from: 15, to: -15, duration: 300, ease: \"easeInOutBack\"})\n.animateProperty(\"sprite\", \"rotation\", { from: 0, to: 30, duration: 250, delay: 200, ease: \"easeOutBack\"})\n.loopProperty(\"sprite\", \"rotation\", { from: 3, to: -3, duration: 1500, ease: \"easeOutQuad\", pingPong: true })\n.persist()\n.zeroSpriteRotation()\n.waitUntilFinished(-500)\n.playIf(() => {\nreturn lightSource == \"lantern\";\n})\n\n.thenDo(function(){\nvar light = {dim: 0, bright: 0};\ntoken.document.update({light});\n\nSequencer.EffectManager.endEffects({ name: `${token.document.name} ${lightSource}`, object: token });\n})\n\n.play()\n\n//Define token light options\nvar light = {dim: 30, bright: 15, alpha:0.25, luminosity: 0.5, color: \"#ffb433\", animation: {type: \"torch\", speed: 4, intensity: 4},attenuation: 0.75, contrast:0.15, shadows:0.2,};\n\nawait token.document.update({light});\n\n} else {\n\nawait Sequencer.EffectManager.endEffects({ name: `${token.document.name} ${lightSource}`, object: token });\n    \nvar light = {dim: 0, bright: 0};\nawait token.document.update({light});\n}",
