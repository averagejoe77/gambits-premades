      "macro": {
        "name": "Touch",
        "img": "icons/magic/fire/explosion-embers-evade-silhouette.webp",
        "type": "script",
        "scope": "global",
        "command": "if(args[0].macroPass === \"postAttackRoll\") {\n    let target = workflow.hitTargets.first();\n    if(!target) return;\n    let checkEffect = target.actor.appliedEffects.some(e => e.name === \"Ignition\");\n    if(checkEffect) return;\n    let effectData = [{\n      \"origin\": macroItem.uuid,\n      \"duration\": {\n        \"seconds\": 99999\n      },\n      \"disabled\": false,\n      \"name\": \"Ignition\",\n      \"img\": macroItem.img,\n      \"changes\": [\n        {\n          \"key\": \"flags.midi-qol.OverTime\",\n          \"mode\": 0,\n          \"value\": \"turn=start,saveRemove=true,damageRoll=1d10,damageType=fire,name=Ignition\",\n          \"priority\": 20\n        }\n      ],\n      \"transfer\": false\n    }];\n\n    await MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: target.actor.uuid, effects: effectData });\n}",
        "author": "roRDywD19pu2VxlC",
