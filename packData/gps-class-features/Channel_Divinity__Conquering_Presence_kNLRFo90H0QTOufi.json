      "macro": {
        "name": "Channel Divinity: Conquering Presence",
        "type": "script",
        "scope": "global",
        "command": "const lastArg = args[args.length - 1];\n\nif (lastArg.failedSaves.length !== 0) {\n  const effectData = game.dfreds.effectInterface.findEffectByName(\"Frightened\").toObject();\n  effectData.duration = { seconds: 60 };\n\n  // Iterate through each failed save and apply the effect\nlastArg.failedSaves.forEach(async (failedSave) => {\n\tconst uuid = failedSave.actor.uuid;\n\tconst hasEffectApplied = await game.dfreds.effectInterface.hasEffectApplied('Frightened', uuid);\n\n\tif (!hasEffectApplied) {\n    await game.dfreds.effectInterface.addEffectWith({\n      effectData,\n      uuid: uuid,\n      origin: item.uuid\n    });\n\t}\n  });\n}",
