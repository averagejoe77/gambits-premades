      "macro": {
        "name": "Fireball",
        "type": "script",
        "scope": "global",
        "command": "const template = fromUuidSync(args[0].templateUuid);\n\nnew Sequence()\n\n.effect()\n.file(\"jb2a.fireball.beam.dark_purple\")\n.atLocation(token)\n.stretchTo(template)\n\n.effect()\n.file(\"jb2a.fire_ring.500px.purple\")\n.atLocation(token)\n.fadeIn(500)\n.fadeOut(500)\n.anchor({x:0.5})\n.scaleToObject(2)\n.duration(4000)\n.rotateTowards(template, {cacheLocation: true})\n.loopProperty(\"sprite\", \"rotation\", { from: 0, to: 360, duration: 1000})\n.scaleOut(0, 4000, {ease: \"easeOutQuint\", delay: -4000})\n.zIndex(2)\n\n.sound()\n\t.file(\"modules/dnd5e-animations/assets/sounds/Spells/Ray/spell-ray-1.mp3\")\n\t.fadeInAudio(500)\n\t.fadeOutAudio(500)\n\n.effect()\n.file(\"jb2a.particles.outward.purple.01.04\")\n.atLocation(token)\n.fadeIn(500)\n.fadeOut(500)\n.anchor({x:0.5})\n.scaleToObject(2)\n.duration(5000)\n.rotateTowards(template, {cacheLocation: true})\n.loopProperty(\"sprite\", \"rotation\", { from: 0, to: 360, duration: 3000})\n.scaleOut(0, 5000, {ease: \"easeOutQuint\", delay: -3000})\n.zIndex(1)\n\n.effect()\n.file(\"jb2a.particles.outward.purple.01.03\")\n.atLocation(token)\n.anchor({x:0.5})\n.scaleToObject(1.75)\n.animateProperty(\"sprite\", \"position.x\", { from: 0, to: -1000, duration: 15000})\n.rotateTowards(template, {cacheLocation: true})\n.scaleIn(0, 500, {ease: \"easeOutQuint\"})\n.duration(6000)\n.playbackRate(2)\n.fadeOut(2000)\n.delay(2000)\n.zIndex(2)\n\n.sound()\n\t.file(\"assets/sounds/fireball2.ogg\")\n\t.fadeInAudio(500)\n\t.fadeOutAudio(500)\n\n.effect()\n.file(\"jb2a.fireball.explosion.dark_purple\")\n.atLocation(template)\n.scaleIn(0, 500, {ease: \"easeOutQuint\"})\n.delay(2100)\n.zIndex(2)\n\n.effect()\n.file(\"jb2a-extras.fireball_blast.01.purple\")\n.attachTo(template)\n.scaleIn(0, 500, {ease: \"easeOutQuint\"})\n.persist()\n.belowTokens()\n.delay(2100)\n\n.play();",
