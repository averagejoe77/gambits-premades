    "dae": {
      "macro": {
        "name": "Roksjas Husk",
        "img": "modules/abyssal-brews-magical-kompendium-vol1/artwork/Roksja%27s%20Husk.webp",
        "type": "script",
        "scope": "global",
        "command": "if(args[0] === \"off\" && args[3] && args[3][\"expiry-reason\"] !== \"effect-deleted\") {\n    Sequencer.EffectManager.endEffects({ name: `${token.id}.RoksjasHusk` });\n\n    let originItem = await fromUuid(args[1])\n\n    if(originItem.system.uses.value === 0) {\n        await effectData.delete();\n        return ui.notifications.warn(\"You have no remaining charges for Roksja's Husk\")\n    }\n    let effectOriginActor = await fromUuid(args[2]);\n    let effectOriginToken = await MidiQOL.tokenForActor(effectOriginActor.uuid);\n\n    let midiSightTest = MidiQOL.canSee(effectOriginToken, token);\n    if (midiSightTest === false) return ui.notifications.warn(\"You can no longer see the target\");\n\n    let dialogId = \"roksjashusk\";\n    let dialogTitleGM = `Waiting for ${effectOriginToken.actor.name}'s selection | ${originItem.name}`;\n    let dialogTitlePrimary = `${effectOriginToken.actor.name} | ${originItem.name}`;\n    let browserUser = game.gps.getBrowserUser({ actorUuid: effectOriginActor.uuid });\n    let result;\n    let initialTimeLeft = 15;\n    let gmUser = game.gps.getPrimaryGM();\n    \n    let dialogContent = `\n        <div class=\"gps-dialog-container\">\n            <div class=\"gps-dialog-section\">\n                <div class=\"gps-dialog-content\">\n                    <div>\n                        <div class=\"gps-dialog-flex\">\n                            <p class=\"gps-dialog-paragraph\">Would you like to use your bonus action to try and keep the creature paralyzed with Roksja's Husk?</p>\n                            <div id=\"image-container\" class=\"gps-dialog-image-container\">\n                                <img id=\"img_${dialogId}\" src=\"${originItem.img}\" class=\"gps-dialog-image\">\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"gps-dialog-button-container\">\n                <button id=\"pauseButton_${dialogId}\" type=\"button\" class=\"gps-dialog-button\">\n                    <i class=\"fas fa-pause\" id=\"pauseIcon_${dialogId}\" style=\"margin-right: 5px;\"></i>Pause\n                </button>\n            </div>\n        </div>\n    `;\n\n    if (MidiQOL.safeGetGameSetting('gambits-premades', 'Mirror 3rd Party Dialog for GMs') && browserUser !== gmUser) {\n    let userDialogArgs = { dialogTitle:dialogTitlePrimary,dialogContent,dialogId,initialTimeLeft,validTokenPrimaryUuid: effectOriginToken.document.uuid,source: \"user\",type: \"multiDialog\", browserUser: browserUser };\n    \n    let gmDialogArgs = { dialogTitle:dialogTitleGM,dialogContent,dialogId,initialTimeLeft,validTokenPrimaryUuid: effectOriginToken.document.uuid,source: \"gm\",type: \"multiDialog\" };\n\n    result = await game.gps.socket.executeAsUser(\"handleDialogPromises\", gmUser, {userDialogArgs, gmDialogArgs});\n} else {\n    result = await game.gps.socket.executeAsUser(\"process3rdPartyReactionDialog\", browserUser, {dialogTitle:dialogTitlePrimary,dialogContent,dialogId,initialTimeLeft,validTokenPrimaryUuid: effectOriginToken.document.uuid,source:gmUser === browserUser ? \"gm\" : \"user\",type:\"singleDialog\"});\n    }\n        \n    const { userDecision, enemyTokenUuid, allyTokenUuid, damageChosen, abilityCheck, source, type } = result;\n\n    if (!userDecision) {\n        return;\n    }\n    else if (userDecision) {\n        await originItem.update({ \"system.uses.value\": originItem.system.uses.value - 1 });\n\n        let saveResult = await game.gps.gpsActivityUse({itemUuid: originItem.uuid, identifier: \"syntheticSave\", targetUuid: token.document.uuid});\n\n        if (saveResult.failedSaves.size !== 0){\n            new Sequence()\n                .effect()\n                    .name(`${token.id}.RoksjasHusk`)\n                    .file(\"jb2a.icon.stun.dark_teal\")\n                    .attachTo(token,{bindVisibility: true})\n                    .scaleToObject(0.7)\n                    .opacity(0.85)\n                    .zIndex(0)\n                    .mask(token)\n                    .persist()\n                    .belowTokens(false)\n                    .fadeIn(500)\n                    .fadeOut(500)\n                    .spriteOffset({x:0.0*token.document.width, y:-0.28*token.document.width}, {gridUnits:true})\n            .play()\n        }\n    }\n}\n\nelse if(args[0] === \"off\" && args[3] && args[3][\"expiry-reason\"] === \"effect-deleted\") {\n    Sequencer.EffectManager.endEffects({ name: `${token.id}.RoksjasHusk` });\n}\n\nelse if(args[0].macroPass === \"postSave\") {\n    let target = workflow.failedSaves.first();\n\n    new Sequence()\n        .effect()\n            .name(`${target.id}.RoksjasHusk`)\n            .file(\"jb2a.icon.stun.dark_teal\")\n            .attachTo(target,{bindVisibility: true})\n            .scaleToObject(0.7)\n            .opacity(0.85)\n            .zIndex(0)\n            .mask(target)\n            .persist()\n            .belowTokens(false)\n            .fadeIn(500)\n            .fadeOut(500)\n            .spriteOffset({x:0.0*target.document.width, y:-0.28*target.document.width}, {gridUnits:true})\n\n    .play()\n}",
        "author": "roRDywD19pu2VxlC",
